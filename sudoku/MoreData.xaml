        <Label Content="Hard level" HorizontalAlignment="Left" Margin="216,19,0,0" VerticalAlignment="Top" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="18" Height="104" Width="106"/>
        <Label Content="Best time" HorizontalAlignment="Left" Margin="327,19,0,0" VerticalAlignment="Top" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="18" Height="104" Width="124"/>
